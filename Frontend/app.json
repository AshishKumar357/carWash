{
  "expo": {
    "name": "CarXWash",
    "slug": "CarXWash",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./Source/assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./Source/assets/SplashIcon.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./Source/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "package": "com.carXwash.carXwashClient"
    },
    "web": {
      "favicon": "./Source/assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow CarXWash to use your location."
        }
      ]
    ]
  }
}
